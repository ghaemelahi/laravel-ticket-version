{
    "name": "binshops/laravel-ticket",
    "description": "A simple ticketing system for Laravel 5.1 to 12.x that integrates smoothly with Laravel's default users and auth system.",
    "type": "library",
    "keywords": [
        "laravel",
        "helpdesk",
        "ticket",
        "support",
        "crm",
        "customer support"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Berry",
            "email": "sam@binshops.com",
            "homepage": "https://github.com/binshops/laravel-ticket"
        },
        {
            "name": "Forked and updated by Ghaem",
            "email": "your@email.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^5.1|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^5.1|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "spatie/laravel-html": "^3.2",
        "yajra/laravel-datatables-oracle": "^9.0|^10.0",
        "jenssegers/date": "^4.0",
        "mews/purifier": "^3.4",
        "doctrine/dbal": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Binshops\\LaravelTicket\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Binshops\\LaravelTicket\\TicketServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
